.cartdrop a i{} 

.cartdrop {
    margin: 0;
   position: relative;
}

.cartdrop a{
	font-size:15px;
	color:#777471;
}

.cartdrop .dropbtn {
 background-color: transparent;
    border: medium none;
    border-radius: 47%;
    color: white;
    cursor: pointer;
    font-size: 14px;
    height: 17px;
    padding: 0;
    width: 17px;
	 color: #ba8f74;
}

.cartdrop .dropdown {
    position: relative;
    display: inline-block;
}

.cartdrop .dropdown-content {
    display: none;
	left: 0;
	/*padding:2px;*/
    position: absolute;
    background-color: #003764;
    min-width:150px;
	/*border-radius:0 0 8px 8px;*/
	 color: #ba8f74;
    box-shadow: 0px 1px 4px 0px rgba(0,0,0,0.2);
    z-index: 1;
	font:11px/13px 'Didact Gothic', sans-serif;
	margin: 0 0 0 3px;


}
.cartdrop .dropdown-content table tr td{
/*	border: 1px solid red;*/
	border:1px solid #002e54;
	padding:5px;
	text-align:center;
	

}
.cartdrop .dropdown-content a {
    color: black;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
}

.cartdrop .dropdown-content a:hover {background-color: #b3b2b0}

.cartdrop .dropdown:hover .dropdown-content {
    display: block;
}

.cartdrop .dropdown:hover .dropbtn {
    background-color: transparent;
}
.cartdrop .caret {
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 4px dashed;
    display: inline-block;
    height: 0;
    margin-left: 2px;
    position: absolute;
    right: -13px;
    top: 10px;
    vertical-align: middle;
    width: 0;
	-ms-transform: rotate(-92deg); /* IE 9 */
    -webkit-transform: rotate(-92deg); /* Safari */
    transform: rotate(-92deg); /* Standard syntax */
}


.dropdown.open .caret-up:after{
	position:absolute;
	content:'';
	background:url(../images/top-arrow.png) left top no-repeat;
	width:8px;
	height:5px;
	top:10px;
	left:20px;
}

.cartdrop .badge {
   background-color:#047fe4;
    border-radius: 10px;
    color: #fff;
    display: inline-block;
    font-size: 9px;
    font-weight: 700;
    left: 10px;
    line-height: 1;
    min-width: 10px;
    padding:3px 6px;
    position: absolute;
    text-align: center;
    top: -4px;
    vertical-align: middle;
    white-space: nowrap;
}


.topmenuPanel{
	background: url(../images/topmenu-mid.png) left top repeat-x;
	/*background:#002e54;
	z-index:3;*/
	height:41px;
	display:block;
	position:relative;
	z-index:2;
	

	 	
}

.topmenuPanel::before{
	background: url(../images/topmenu-left.png) left top no-repeat;
	height:41px;
	content:"";
	left:-62px;
	width:62px;
	position:absolute;
}

.topmenuPanel::after{
	background: url(../images/topmenu-right.png) left top no-repeat;
	height:48px;
	content:"";
	right:-75px;
	width:75px;
	position:absolute;
}

ul.topnav li{ list-style-type: none; float: left; margin: 0 0 0 10px;} 


.nav-logo-section{
