/*================Top search==================*/


.overlay {
    height: 100%;
    width: 0;
    position: fixed;
    z-index: 999;
    top: 0;
    left: 0;
   /* background-color: rgb(0,0,0);*/
    background-color: rgba(0,0,0, 0.7);
    overflow-x: hidden;
    transition: 0.5s;
}

.overlay-content {
    position: relative;
    top: 25%;
    width: 100%;
    text-align: center;
    margin-top: 30px;
}

.overlay a {
    padding: 8px;
    text-decoration: none;
    font-size: 36px;
    color: #818181;
    display: block;
    transition: 0.3s;
}

.overlay a:hover, .overlay a:focus {
    color: #f1f1f1;
}

.overlay .closebtn {
    position: absolute;
    top: 20px;
    right: 45px;
    font-size: 60px;
}


.form-search {
    margin: 0 10px 10px 0;
}

.form-search-wrapper {
    margin-right: 125px;
    position: relative;
}
.form-search-field {
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.9) inset;
    width: 100%;
}

.form-search-wrapper label{
	font-size:44px;
	color:#fff;
	text-align:left;
	font-family: "Open Sans",sans-serif;
	text-transform:uppercase;
}
.searchPanel{
	width:100%;
	position:relative;	
}

.form-search-field, .form-search-submit {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: white;
    border-color: #aaa #ccc #ccc;
    border-image: none;
    border-style: solid;
    border-width: 1px;
    box-sizing: border-box;
    color: #666;
    display: block;
    font-family: inherit;
    font-feature-settings: inherit;
    font-kerning: inherit;
    font-language-override: inherit;
    font-size: inherit;
    font-size-adjust: inherit;
    font-stretch: inherit;
    font-style: inherit;
    font-synthesis: inherit;
    font-variant: inherit;
    font-weight: inherit;
    line-height: normal;
    outline: medium none;
    padding: 10px;
    text-align: left;
    text-shadow: none;
}

.form-search-submit {
    background: #003766 url("data:image/png;base64,") no-repeat scroll 15px 50%;
    border-color: #002e54 #002e54 #002e54;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.2);
    color: white;
    cursor: pointer;
    left: 100%;
    margin-left: 10px;
    padding-left: 40px;
    padding-right: 0;
    position: absolute;
    top: 0;
    width: 115px;
}




@media screen and (max-height: 450px) {
  .overlay a {font-size: 20px}
  .overlay .closebtn {
    font-size: 60px;
    top: 15px;
    right: 35px;
  }
}

/*===================Top search End=====================*/







